<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="application.classes.GroupPosts"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10" fx:id="rootPane">
         
    <Label text = "Posts for Group" GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.halignment="LEFT" fx:id="sceneTitle"/>
    <Label GridPane.rowIndex="1" GridPane.columnIndex="1" fx:id="questionDisplay" />
    <Button GridPane.rowIndex="2" GridPane.columnIndex="2" MaxWidth="200" text="Finish" onAction="#complete" />
    <Label GridPane.rowIndex="3" GridPane.halignment="CENTER" fx:id="actionLabel" />
</GridPane>